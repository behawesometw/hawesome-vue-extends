{
  "name": "hawesome-vue-extends",
  "version": "0.0.0-development",
  "description": "hawesome-vue-extends",
  "main": "./dist/hawesome-vue-extends.common.js",
  "types": "index.d.ts",
  "typing": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts",
    "lib"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./dev/demoEntry.js --open",
    "build-demo-site": "vue-cli-service build --dest demoSite ./dev/demoEntry.js",
    "build": "vue-cli-service build --target lib --name hawesome-vue-extends src/plugin.js",
    "lint": "vue-cli-service lint",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vuetify": "^2.1.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.4",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0",
    "semantic-release": "^17.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
